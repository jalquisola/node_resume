extends layout

block content
  div(id="section-topbar")
    div(id="topbar-inner")
      .container
        .row
          .dropdown
            ul(id="nav", class="nav")
              li(class="menu-item")
                a(class="smoothScroll", href="#about", title="About")
                  i(class="icon-user")
              li(class="menu-item")
                a(class="smoothScroll", href="#resume", title="Resume")
                  i(class="icon-file")
              li(class="menu-item")
                a(class="smoothScroll", href="#work", title="Works")
                  i(class="icon-briefcase")
              li(class="menu-item")
