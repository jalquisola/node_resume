  div(id="skillswrap")
    div(class="container")
      div(class="row")
        div(class="col-lg-2 col-lg-offset-1")
          h5 SKILLS

        - each obj, index in skills
          div(class="col-lg-3 #{index>0 && index%3==0 ? 'col-lg-offset-3' : '' } centered")
            canvas(id=obj.html_id,height="130", width="130")
            p
              strong=obj.label
              br
              - if(obj.info.length > 0)
                small (#{obj.info.join(', ')})
            script.
                generate_pie_graph(!{JSON.stringify(obj)})

